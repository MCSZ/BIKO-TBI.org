ns1:BIKO_000004 a owl:Class ;
    rdfs:label "Material entity" ;
    skos:definition "A material entity is a physical entity that is spatially extended, exists as a whole at any point in time and has mass." ;
    rdfs:subClassOf owl:Thing ;
    rdfs:isDefinedBy <http://semanticscience.org/resource/SIO_000004> ;
    ns4:created_by "MCSZ" ;
    ns4:creation_date "2022-09-29T18:34:54.323158Z" ;
    rdfs:seeAlso <http://semanticscience.org/resource/SIO_000004> .

ns1:BIKO_000005 a owl:Class ;
    rdfs:label "Process" ;
    skos:definition "An entity that is identifiable only through the unfolding of time, has temporal parts, and unless otherwise specified/predicted, cannot be identified from any instant of time in which it exists." ;
    rdfs:subClassOf owl:Thing ;
    rdfs:isDefinedBy <https://semanticscience.org/resource/SIO_000006.rdf> ;
    ns4:created_by "MCSZ" ;
    ns4:creation_date "2022-09-29T18:35:11.362856Z" ;
    rdfs:seeAlso <http://purl.obolibrary.org/obo/BFO_0000015>,
        <https://semanticscience.org/resource/SIO_000006.rdf> .

ns1:BIKO_000007 a owl:Class ;
    rdfs:label "Injury" ;
    skos:definition "Damage inflicted on the body as the direct or indirect result of an external force, with or without disruption of structural continuity." ;
    rdfs:subClassOf ns1:BIKO_000005 ;
    rdfs:isDefinedBy <http://purl.obolibrary.org/obo/NCIT_C3671> ;
    ns1:BIKO_000009 "trauma",
        "wound" ;
    ns4:created_by "MCSZ" ;
    ns4:creation_date "2022-09-29T18:57:39.376802Z" .

