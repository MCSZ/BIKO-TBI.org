ns1:BIKO_000086 a owl:Class ;
    rdfs:label "Organism" ;
    skos:definition "A material entity that is an individual living system, such as animal, plant, bacteria or virus, that is capable of replicating or reproducing, growth and maintenance in the right environment. An organism may be unicellular or made up, like humans, of many billions of cells divided into specialized tissues and organs. E.g Drosophila melanogaster" ;
    rdfs:subClassOf ns1:BIKO_000004 ;
    rdfs:isDefinedBy <http://en.wikipedia.org/wiki/Organism>,
        <http://purl.obolibrary.org/obo/OBI_0100026> ;
    ns4:created_by "MCSZ" ;
    ns4:creation_date "2022-10-05T19:57:26.608424Z" .

ns1:BIKO_000087 a owl:Class ;
    rdfs:label "Biological process" ;
    skos:definition "Any process specifically pertinent to the functioning of integrated living units: cells, tissues, organs, and organisms. A process is a collection of molecular events with a defined beginning and end." ;
    rdfs:subClassOf ns1:BIKO_000005 ;
    rdfs:isDefinedBy <https://bioportal.bioontology.org/ontologies/RB/?p=classes&conceptid=http%3A%2F%2Fregenbase.org%2Fontology%23RB_0000353> ;
    ns4:created_by "MCSZ" ;
    ns4:creation_date "2022-10-06T18:21:47.085481Z" ;
    rdfs:seeAlso <http://purl.obolibrary.org/obo/GO_0008150>,
        <http://regenbase.org/ontology#RB_0008000> .

ns1:BIKO_000088 a owl:Class ;
    rdfs:label "Role" ;
    skos:definition "A role is particular behaviour which a material entity may exhibit." ;
    rdfs:subClassOf ns1:BIKO_000002 ;
    rdfs:isDefinedBy <http://purl.obolibrary.org/obo/PATO_0001018> ;
    ns4:created_by "MCSZ" ;
    ns4:creation_date "2022-10-11T05:20:00.995215Z" ;
    rdfs:seeAlso <http://purl.obolibrary.org/obo/BFO_0000023>,
        <http://purl.obolibrary.org/obo/PATO_0001018>,
        <http://regenbase.org/ontology#RB_0000485> .

ns1:BIKO_000089 a owl:Class ;
    rdfs:label "Study group" ;
    skos:definition "A study population role where the bearer is a population of material entities and the role is realized in the implementation of a study design wherein the entities bearing the study population role are observed or subjected to intervention according to the study design and are biological replicates, i.e. they receive the same treatment under the protocol" ;
    rdfs:subClassOf ns1:BIKO_000088 ;
    rdfs:isDefinedBy <https://orcid.org/0000-0002-2308-8813> ;
    ns4:created_by "MCSZ" ;
    ns4:creation_date "2022-10-11T05:20:44.965035Z" ;
    rdfs:seeAlso <http://purl.obolibrary.org/obo/OBI_0000174> .

ns1:BIKO_000090 a owl:Class ;
    rdfs:label "Control group" ;
    skos:definition "A group separated from the rest of the experiment where the independent variable(s) being tested cannot influence the results. This isolates the independent variable's effects on the experiment and can help rule out alternate explanations of the experimental results." ;
    rdfs:subClassOf ns1:BIKO_000089 ;
    rdfs:isDefinedBy <https://bioportal.bioontology.org/ontologies/RB/?p=classes&conceptid=http%3A%2F%2Fregenbase.org%2Fontology%23RB_0000250> ;
    ns4:created_by "MCSZ" ;
    ns4:creation_date "2022-10-11T05:21:01.827731Z" ;
    rdfs:seeAlso <http://regenbase.org/ontology#RB_0000250> .

ns1:BIKO_000091 a owl:Class ;
    rdfs:label "Experimental group" ;
    skos:definition "A group of samples/individals that is exposed to (one of) the independent variable (s) being tested." ;
    rdfs:subClassOf ns1:BIKO_000089 ;
    rdfs:isDefinedBy <https://bioportal.bioontology.org/ontologies/RB/?p=classes&conceptid=http%3A%2F%2Fregenbase.org%2Fontology%23RB_0000304> ;
    ns4:created_by "MCSZ" ;
    ns4:creation_date "2022-10-11T05:24:24.029587Z" ;
    rdfs:seeAlso <http://regenbase.org/ontology#RB_0000304> .

ns1:BIKO_000092 a owl:Class ;
    rdfs:label "Injury control group" ;
    skos:definition "A control for experimental injury in which the animal is not injured." ;
    rdfs:subClassOf ns1:BIKO_000090 ;
    rdfs:isDefinedBy <https://bioportal.bioontology.org/ontologies/RB/?p=classes&conceptid=http%3A%2F%2Fregenbase.org%2Fontology%23RB_0000345> ;
    ns4:created_by "MCSZ" ;
    ns4:creation_date "2022-10-11T05:25:27.821405Z" ;
    rdfs:seeAlso <http://regenbase.org/ontology#RB_0000345> .

ns1:BIKO_000093 a owl:Class ;
    rdfs:label "Naive control group" ;
    skos:definition "A control for experimental injury in which the animal is not injured, and does not receive anaesthesia or post-operative treatments such as antibiotics, analgesics or injected fluids." ;
    rdfs:subClassOf ns1:BIKO_000092 ;
    rdfs:isDefinedBy <https://bioportal.bioontology.org/ontologies/RB/?p=classes&conceptid=http%3A%2F%2Fregenbase.org%2Fontology%23RB_0008014> ;
    ns4:created_by "MCSZ" ;
    ns4:creation_date "2022-10-11T05:25:44.513132Z" ;
    rdfs:seeAlso <http://regenbase.org/ontology#RB_0008014> .

ns1:BIKO_000094 a owl:Class ;
    rdfs:label "Sham surgery control group" ;
    skos:definition "A control for a surgical procedure in which aspects of the procedure other than the injury intended to induce the neurological effect itself are mimicked, including anaesthesia, surgical preparation (e.g., dissection, nerve exposure, burr hole) and post-operative treatments." ;
    rdfs:subClassOf ns1:BIKO_000092 ;
    rdfs:isDefinedBy <https://bioportal.bioontology.org/ontologies/RB/?p=classes&conceptid=http%3A%2F%2Fregenbase.org%2Fontology%23RB_0000278> ;
    ns4:created_by "MCSZ" ;
    ns4:creation_date "2022-10-11T05:27:40.23012Z" ;
    rdfs:seeAlso <http://regenbase.org/ontology#RB_0000278> .

ns1:BIKO_000095 a owl:Class ;
    rdfs:label "Negative treatment control group" ;
    skos:definition "A control in which a perturbagen that is known to consistently produce no response is used, or in which components of the treatment other than the active perturbagen are used (such as vehicle, vector, inactive compound, etc). This gives the investigators one or more possible baselines to compare to unknowns and can also be used to normalize data across samples as well as experiments done on different days." ;
    rdfs:subClassOf ns1:BIKO_000091 ;
    rdfs:isDefinedBy <https://bioportal.bioontology.org/ontologies/RB/?p=classes&conceptid=http%3A%2F%2Fregenbase.org%2Fontology%23RB_0000107> ;
    ns4:created_by "MCSZ" ;
    ns4:creation_date "2022-10-11T05:29:21.16139Z" ;
    rdfs:seeAlso <http://regenbase.org/ontology#RB_0000107> .

ns1:BIKO_000096 a owl:Class ;
    rdfs:label "Positive treatment control group" ;
    skos:definition "A control in which a perturbagen that is known to produce a consistent response is used. This gives the investigators confidence that the assay is performing as expected." ;
    rdfs:subClassOf ns1:BIKO_000091 ;
    rdfs:isDefinedBy <https://bioportal.bioontology.org/ontologies/RB/?p=classes&conceptid=http%3A%2F%2Fregenbase.org%2Fontology%23RB_0000194> ;
    ns4:created_by "MCSZ" ;
    ns4:creation_date "2022-10-11T05:30:18.954329Z" ;
    rdfs:seeAlso <http://regenbase.org/ontology#RB_0000194> .

ns1:BIKO_000097 a owl:Class ;
    rdfs:label "Inactive treatment control group" ;
    skos:definition "A control for experimental treatment in which an inactive form of the treatment is substituted for the active form, such as a scrambled siRNA, a plasmid/virus with inactive insert, or an inactive but related chemical compound." ;
    rdfs:subClassOf ns1:BIKO_000095 ;
    rdfs:isDefinedBy <https://bioportal.bioontology.org/ontologies/RB/?p=classes&conceptid=http%3A%2F%2Fregenbase.org%2Fontology%23RB_0008011> ;
    ns4:created_by "MCSZ" ;
    ns4:creation_date "2022-10-11T05:31:20.322234Z" ;
    rdfs:seeAlso <http://regenbase.org/ontology#RB_0008011> .

ns1:BIKO_000098 a owl:Class ;
    rdfs:label "No treatment control group" ;
    skos:definition "A control for experimental treatment in which neither the perturbagen nor its vehicle is added." ;
    rdfs:subClassOf ns1:BIKO_000095 ;
    rdfs:isDefinedBy <https://bioportal.bioontology.org/ontologies/RB/?p=classes&conceptid=http%3A%2F%2Fregenbase.org%2Fontology%23RB_0008010> ;
    ns4:created_by "MCSZ" ;
    ns4:creation_date "2022-10-11T05:32:12.433711Z" ;
    rdfs:seeAlso <http://regenbase.org/ontology#RB_0008010> .

ns1:BIKO_000099 a owl:Class ;
    rdfs:label "Vehicle control group" ;
    skos:definition "A control for experimental treatment in which only the vehicle used to dissolve the treatment chemical is delivered at the same concentration and by the same route as in the experimental treatment." ;
    rdfs:subClassOf ns1:BIKO_000095 ;
    rdfs:isDefinedBy <https://bioportal.bioontology.org/ontologies/RB/?p=classes&conceptid=http%3A%2F%2Fregenbase.org%2Fontology%23RB_0000036> ;
    ns4:created_by "MCSZ" ;
    ns4:creation_date "2022-10-11T05:33:02.022053Z" ;
    rdfs:seeAlso <http://regenbase.org/ontology#RB_0000036> .

ns1:BIKO_000100 a owl:Class ;
    rdfs:label "Measurement" ;
    skos:definition "A quantitative description that reflects the magnitude of some attribute." ;
    rdfs:subClassOf ns1:BIKO_000003 ;
    rdfs:isDefinedBy <http://semanticscience.org/resource/SIO_000070> ;
    ns4:created_by "MCSZ" ;
    ns4:creation_date "2022-10-11T05:35:39.584142Z" ;
    rdfs:seeAlso <http://regenbase.org/ontology#RB_0000437> .

ns1:BIKO_000103 a owl:Class ;
    rdfs:label "Neuroanatomical assessment" ;
    skos:definition "A quantitative or qualitative assessment of a neuroanatomical property of an experimental subject." ;
    rdfs:subClassOf ns1:BIKO_000040 ;
    rdfs:isDefinedBy <https://bioportal.bioontology.org/ontologies/RB/?p=classes&conceptid=http%3A%2F%2Fregenbase.org%2Fontology%23RB_0008028> ;
    ns4:created_by "MCSZ" ;
    ns4:creation_date "2022-10-11T05:37:45.8449Z" .

ns1:BIKO_000104 a owl:Class ;
    rdfs:label "Physiological assessment" ;
    skos:definition "A quantitative assessment of a physiological property of an experimental subject." ;
    rdfs:subClassOf ns1:BIKO_000040 ;
    rdfs:isDefinedBy <https://bioportal.bioontology.org/ontologies/RB/?p=classes&conceptid=http%3A%2F%2Fregenbase.org%2Fontology%23RB_0008035> ;
    ns4:created_by "MCSZ" ;
    ns4:creation_date "2022-10-11T05:38:02.359181Z" .

ns1:BIKO_000105 a owl:Class ;
    rdfs:label "Parietal cortex" ;
    rdfs:subClassOf ns1:BIKO_000148 ;
    ns4:created_by "MCSZ" ;
    ns4:creation_date "2022-11-09T14:13:23.03551Z" .

ns1:BIKO_000117 a owl:Class ;
    rdfs:label "Cavity size" ;
    skos:definition "A quantitative assessment of the size of a cavity created by a central nervous system injury" ;
    rdfs:subClassOf ns1:BIKO_000103 ;
    rdfs:isDefinedBy <https://bioportal.bioontology.org/ontologies/RB/?p=classes&conceptid=http%3A%2F%2Fregenbase.org%2Fontology%23RB_0008048> ;
    ns4:created_by "MCSZ" ;
    ns4:creation_date "2022-10-12T18:01:48.691286Z" .

ns1:BIKO_000118 a owl:Class ;
    rdfs:label "Lesion volume" ;
    skos:definition "A quantitative assessment of the volume of a lesion created by a central nervous system injury." ;
    rdfs:subClassOf ns1:BIKO_000103 ;
    rdfs:isDefinedBy <https://bioportal.bioontology.org/ontologies/RB/?p=classes&conceptid=http%3A%2F%2Fregenbase.org%2Fontology%23RB_0008029> ;
    ns4:created_by "MCSZ" ;
    ns4:creation_date "2022-10-12T18:02:15.820411Z" .

ns1:BIKO_000119 a owl:Class ;
    rdfs:label "Activation" ;
    skos:definition "An increase in the frequency, rate or extent of a biological process." ;
    rdfs:subClassOf ns1:BIKO_000087 ;
    rdfs:isDefinedBy <https://bioportal.bioontology.org/ontologies/RB/?p=classes&conceptid=http%3A%2F%2Fregenbase.org%2Fontology%23RB_0000348> ;
    ns4:created_by "MCSZ" ;
    ns4:creation_date "2022-10-12T18:04:43.275888Z" .

ns1:BIKO_000120 a owl:Class ;
    rdfs:label "Atrophy" ;
    skos:definition "Wasting or decrease in size of a cell, tissue, or organ as a result of disuse, injury, or nutritional deficiency." ;
    rdfs:subClassOf ns1:BIKO_000087 ;
    rdfs:isDefinedBy <https://bioportal.bioontology.org/ontologies/RB/?p=classes&conceptid=http%3A%2F%2Fregenbase.org%2Fontology%23RB_0000324> ;
    ns4:created_by "MCSZ" ;
    ns4:creation_date "2022-10-12T18:05:13.997441Z" .

ns1:BIKO_000121 a owl:Class ;
    rdfs:label "Autophagy" ;
    skos:definition "The digestion of cellular components by the cell's own lysosomes. This can proceed by formation of intracellular membrane compartments that fuse with lysosomes or by direct import of the cell's components into lysosomes." ;
    rdfs:subClassOf ns1:BIKO_000087 ;
    rdfs:isDefinedBy <https://bioportal.bioontology.org/ontologies/RB/?p=classes&conceptid=http%3A%2F%2Fregenbase.org%2Fontology%23RB_0000171> ;
    ns4:created_by "MCSZ" ;
    ns4:creation_date "2022-10-12T18:05:53.532129Z" .

ns1:BIKO_000122 a owl:Class ;
    rdfs:label "Cell death" ;
    skos:definition "Any biological process that results in permanent cessation of all vital functions of a cell." ;
    rdfs:subClassOf ns1:BIKO_000087 ;
    rdfs:isDefinedBy <https://bioportal.bioontology.org/ontologies/RB/?p=classes&conceptid=http%3A%2F%2Fregenbase.org%2Fontology%23RB_0000152> ;
    ns4:created_by "MCSZ" ;
    ns4:creation_date "2022-10-12T18:06:49.8081Z" .

ns1:BIKO_000123 a owl:Class ;
    rdfs:label "Physical quality" ;
    skos:definition "A quality of a physical entity that exists through action of continuants at the physical level of organisation in relation to other entities." ;
    rdfs:subClassOf ns1:BIKO_000064 ;
    rdfs:isDefinedBy <http://purl.obolibrary.org/obo/PATO_0001018> ;
    ns1:BIKO_000009 "relational physical quality" ;
    ns4:created_by "MCSZ" ;
    ns4:creation_date "2022-10-19T19:14:27.940056Z" ;
    rdfs:seeAlso <http://purl.obolibrary.org/obo/PATO_0001018> .

ns1:BIKO_000124 a owl:Class ;
    rdfs:label "Cell cycle" ;
    skos:definition "The period between the formation of a cell by division of its mother cell and the time when the cell itself divides to form two daughter cells." ;
    rdfs:subClassOf ns1:BIKO_000087 ;
    rdfs:isDefinedBy <https://bioportal.bioontology.org/ontologies/RB/?p=classes&conceptid=http%3A%2F%2Fregenbase.org%2Fontology%23RB_0000208> ;
    ns4:created_by "MCSZ" ;
    ns4:creation_date "2022-10-12T18:09:46.306276Z" .

ns1:BIKO_000125 a owl:Class ;
    rdfs:label "Apoptosis" ;
    skos:definition "A form of programmed cell death that begins when a cell receives internal or external signals that trigger the activity of proteolytic caspases, proceeds through a series of characteristic stages typically including rounding-up of the cell, retraction of pseudopodes, reduction of cellular volume (pyknosis), chromatin condensation, nuclear fragmentation (karyorrhexis), and plasma membrane blebbing (but maintenance of its integrity until the final stages of the process), and ends with the death of the cell." ;
    rdfs:subClassOf ns1:BIKO_000122 ;
    rdfs:isDefinedBy <https://bioportal.bioontology.org/ontologies/RB/?p=classes&conceptid=http%3A%2F%2Fregenbase.org%2Fontology%23RB_0000040> ;
    ns4:created_by "MCSZ" ;
    ns4:creation_date "2022-10-12T18:10:33.841826Z" .

ns1:BIKO_000126 a owl:Class ;
    rdfs:label "Autophagic cell death" ;
    skos:definition "Cell death induced by autophagy." ;
    rdfs:subClassOf ns1:BIKO_000122 ;
    rdfs:isDefinedBy <https://bioportal.bioontology.org/ontologies/RB/?p=classes&conceptid=http%3A%2F%2Fregenbase.org%2Fontology%23RB_0000007> ;
    ns4:created_by "MCSZ" ;
    ns4:creation_date "2022-10-12T18:11:15.965626Z" .

ns1:BIKO_000127 a owl:Class ;
    rdfs:label "Glial cell death" ;
    skos:definition "A biological process that results in permanent cessation of all vital functions of a glial cell." ;
    rdfs:subClassOf ns1:BIKO_000122 ;
    ns4:created_by "MCSZ" ;
    ns4:creation_date "2022-10-12T18:11:56.146626Z" .

ns1:BIKO_000128 a owl:Class ;
    rdfs:label "Necrotic cell death" ;
    skos:definition "Cell death induced by necrosis." ;
    rdfs:subClassOf ns1:BIKO_000122 ;
    rdfs:isDefinedBy <https://bioportal.bioontology.org/ontologies/RB/?p=classes&conceptid=http%3A%2F%2Fregenbase.org%2Fontology%23RB_0000286> ;
    ns4:created_by "MCSZ" ;
    ns4:creation_date "2022-10-12T18:12:40.156387Z" .

ns1:BIKO_000129 a owl:Class ;
    rdfs:label "Neuronal cell death" ;
    skos:definition "Any biological process that results in permanent cessation of all vital functions of a neuronal cell." ;
    rdfs:subClassOf ns1:BIKO_000122 ;
    rdfs:isDefinedBy <https://bioportal.bioontology.org/ontologies/RB/?p=classes&conceptid=http%3A%2F%2Fregenbase.org%2Fontology%23RB_0000354> ;
    ns4:created_by "MCSZ" ;
    ns4:creation_date "2022-10-12T18:13:16.697697Z" .

ns1:BIKO_000130 a owl:Class ;
    rdfs:label "Degeneration" ;
    skos:definition "The process whereby a cell, tissue, or organ changes to a less specialized or functionally less active form." ;
    rdfs:subClassOf ns1:BIKO_000087 ;
    rdfs:isDefinedBy <https://bioportal.bioontology.org/ontologies/RB/?p=classes&conceptid=http%3A%2F%2Fregenbase.org%2Fontology%23RB_0000264> ;
    ns4:created_by "MCSZ" ;
    ns4:creation_date "2022-10-12T18:14:15.821259Z" .

ns1:BIKO_000131 a owl:Class ;
    rdfs:label "axon degeneration" ;
    skos:definition "The death and subsequent loss of axons following disease, insult, or injury." ;
    rdfs:subClassOf ns1:BIKO_000130 ;
