ns1:BIKO_000014 a owl:Class ;
    rdfs:label "Secondary brain injury" ;
    skos:definition "The biochemical, cellular and physiological events that occur during primary injury that often progress into delayed and prolonged secondary damages." ;
    rdfs:subClassOf ns1:BIKO_000154 ;
    rdfs:isDefinedBy <https://doi.org/10.3389/fncel.2019.00528> ;
    ns4:created_by "MCSZ" ;
